enable
  configure terminal
    vlan 12
      name Proprio
  exit
  !Proprio
  interface fa0/1
    no shutdown
    switchport mode access
    switchport access vlan 12
  !Ports non-utilis√©s
  interface range fa0/2 - 23
    shutdown
  !Connexion au routeur
  interface fa0/24
    no shutdown
    switchport mode trunk
    switch trunk allowed vlan all

  exit
  exit
  copy running-config startup-config

exit
