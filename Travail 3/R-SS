enable
  configure terminal
    interface fa0/0
      ip nat inside
      no shutdown
    !Proprio
    interface fa0/0.1
      no shutdown
      encapsulation dot1q
      ip address 192.168.10.0
      ip access-group 110 in
    !Admin
    interface fa0/0.2
      no shutdown
      encapsulation dot2q
      ip address 192.168.20.0
      ip access-group 120 in
    !Prod
    interface fa0/0.3
      no shutdown
      encapsulation dot3q
      ip address 192.168.30.0
      ip access-group 130 in
    !RD
    interface fa0/0.4
      no shutdown
      encapsulation dot4q
      ip address 192.168.40.0
      ip access-group 140 in
    !Info
    interface fa0/0.5
      no shutdown
      encapsulation dot5q
      ip address 192.168.50.0
      ip access-group 150 in
    !Servers
    interface fa0/0.6
      no shutdown
      encapsulation dot6q
      ip address 192.168.60.0
      ip access-group 160 in
    interface fa0/1
      ip address 20.20.20.1 255.255.255.0
      ip nat outside
      no shutdown
    ip dhcp pool Proprio
  		no default-router
  		no dns-server
  		network 192.168.10.0 255.255.255.0
  		default-router 192.168.10.1
  		dns-server 192.168.60.10
    ip dhcp pool Admin
  		no default-router
  		no dns-server
  		network 192.168.20.0 255.255.255.0
  		default-router 192.168.20.1
  		dns-server 192.168.60.10
    ip dhcp pool Prod
  		no default-router
  		no dns-server
  		network 192.168.30.0 255.255.255.0
  		default-router 192.168.30.1
  		dns-server 192.168.60.10
    ip dhcp pool RD
  		no default-router
  		no dns-server
  		network 192.168.40.0 255.255.255.0
  		default-router 192.168.40.1
  		dns-server 192.168.60.10
    ip dhcp pool Proprio
  		no default-router
  		no dns-server
  		network 192.168.50.0 255.255.255.0
  		default-router 192.168.50.1
  		dns-server 192.168.60.10
    exit
    ip nat pool NatPool 20.20.20.1 20.20.20.1 netmask 255.255.255.0
    ip nat inside source list 10 pool NatPool overload
  exit
exit
